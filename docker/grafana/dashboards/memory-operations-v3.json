      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "noValue": "0%",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "#73BF69"
              },
              {
                "value": 20,
                "color": "#FADE2A"
              },
              {
                "value": 50,
                "color": "#F2495C"
