    {
      "id": 3,
      "type": "stat",
      "title": "Duplicates Detected (1h)",
      "description": "Total duplicates skipped in the last hour",
      "gridPos": {
        "x": 0,
        "y": 9,
        "w": 8,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(changes(aimemory_dedup_events_created{action=\"skipped_duplicate\", project=~\"$project\"}[$__range]))",
          "refId": "A",
          "legendFormat": "Duplicates"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "noValue": "0",
          "color": {
            "mode": "fixed",
            "fixedColor": "#FADE2A"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "area"
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
