    {
      "id": 4,
      "type": "stat",
      "title": "Dedup Check p95 Latency",
      "description": "NFR-P4 Target: <100ms",
      "gridPos": {
        "x": 8,
        "y": 9,
        "w": 8,
        "h": 4
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(aimemory_dedup_check_duration_seconds_bucket{project=~\"$project\"}) by (le))",
          "refId": "A",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "noValue": "No data",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "#73BF69"
              },
              {
                "value": 0.08,
                "color": "#FADE2A"
              },
              {
                "value": 0.1,
                "color": "#F2495C"
              }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "area"
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
    {
      "id": 5,
