      "description": "Percentage of captures that were duplicates",
      "gridPos": {
        "x": 16,
        "y": 9,
        "w": 8,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(changes(aimemory_dedup_events_created{action=\"skipped_duplicate\", project=~\"$project\"}[$__range])) / (sum(changes(aimemory_dedup_events_created{project=~\"$project\"}[$__range])) > 0) * 100 or vector(0)",
          "refId": "A",
          "legendFormat": "Dedup %"
        }
