{
  "uid": "ai-memory-nfr-performance-v3",
  "title": "NFR Performance Overview (V3)",
  "tags": [
    "ai-memory",
    "nfr",
    "performance",
    "v3",
    "slo"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "10s",
      "30s",
      "1m",
      "5m",
      "15m"
    ]
  },
  "templating": {
    "list": [
      {
        "name": "project",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "query": "label_values(aimemory_hook_duration_seconds_count, project)",
        "refresh": 2,
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "hook_type",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "query": "label_values(aimemory_hook_duration_seconds_count, hook_type)",
        "refresh": 2,
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "panels": [
    {
      "id": 100,
      "type": "row",
      "title": "NFR Status Summary",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 1
      },
      "collapsed": false
    },
    {
      "id": 1,
      "type": "stat",
      "title": "NFR-P1: Hook Latency",
      "description": "**Target:** <500ms (p95)\n**Metric:** aimemory_hook_duration_seconds\n**Colors:** Green <400ms | Yellow 400-500ms | Red >500ms",
      "gridPos": {
        "x": 0,
        "y": 1,
        "w": 4,
        "h": 4
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(aimemory_hook_duration_seconds_bucket{project=~\"$project\"}[5m])) by (le))",
          "refId": "A",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "noValue": "No data",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "#73BF69"
              },
              {
                "value": 0.4,
                "color": "#FADE2A"
              },
              {
                "value": 0.5,
                "color": "#F2495C"
              }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "textMode": "value",
        "colorMode": "background",
        "graphMode": "area"
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      }
    },
